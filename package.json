{
    "name": "planet-vector-tile",
    "version": "0.0.0",
    "main": "electron/main.js",
    "types": "index.d.ts",
    "napi": {
        "name": "planet-vector-tile",
        "triples": {
            "additional": [
                "aarch64-apple-darwin"
            ]
        }
    },
    "license": "MIT",
    "devDependencies": {
        "@napi-rs/cli": "^2.11.4",
        "ava": "^4.3.0",
        "prettier": "2.7.1",
        "typescript": "^4.8.4"
    },
    "engines": {
        "node": ">= 16",
        "npm": ">=8"
    },
    "scripts": {
        "start": "electron .",
        "artifacts": "napi artifacts",
        "build": "napi build --platform --release && tsc -p tsconfig.json && prettier --write .",
        "build:debug": "napi build --platform && tsc -p tsconfig.json && prettier --write .",
        "prepublishOnly": "napi prepublish -t npm",
        "test": "ava",
        "version": "napi version",
        "format": "prettier --write ."
    },
    "ava": {
        "files": [
            "test/**/*",
            "!maplibre-gl-js"
        ]
    },
    "dependencies": {
        "@mapbox/point-geometry": "^0.1.0",
        "@types/mapbox__vector-tile": "^1.3.0",
        "electron": "20.3.2",
        "flatbuffers": "^2.0.7",
        "geojson": "^0.5.0"
    }
}
