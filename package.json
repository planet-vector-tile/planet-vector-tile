{
    "name": "planet-vector-tile",
    "version": "0.0.0",
    "main": "index.js",
    "types": "index.d.ts",
    "napi": {
        "name": "planet-vector-tile",
        "triples": {
            "additional": [
                "aarch64-apple-darwin"
            ]
        }
    },
    "license": "MIT",
    "devDependencies": {
        "@napi-rs/cli": "^2.11.4",
        "ava": "^4.3.0",
        "prettier": "2.7.1",
        "rollup": "^3.2.3",
        "typescript": "^4.8.4"
    },
    "engines": {
        "node": ">= 16",
        "npm": ">=8"
    },
    "scripts": {
        "start": "cd electron && npm start",
        "artifacts": "napi artifacts",
        "build:debug": "npm i && napi build --platform --js && npm run build:ts",
        "build": "npm i && napi build --platform --release && npm run build:ts",
        "build:ts": "tsc -p tsconfig.json && prettier --write . && npm run rollup",
        "rollup": "rollup --config rollup.config.mjs",
        "clean": "rm -rf index* node_modules package-lock.json dist/* && cargo clean",
        "prepublishOnly": "napi prepublish -t npm",
        "test": "ava",
        "t": "npm run build && npm run test",
        "version": "napi version",
        "format": "prettier --write ."
    },
    "ava": {
        "files": [
            "test/**/*",
            "!maplibre-gl-js"
        ]
    },
    "dependencies": {
        "@mapbox/point-geometry": "^0.1.0",
        "@types/mapbox__vector-tile": "^1.3.0",
        "flatbuffers": "^2.0.7",
        "geojson": "^0.5.0"
    }
}
